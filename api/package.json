{
  "name": "dpa-backend-std",
  "version": "1.3.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --max-warnings=0",
    "prisma:gen": "prisma generate --schema ./prisma/schema.prisma",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:dev": "prisma migrate dev --name init",
    "seed": "ts-node ./scripts/seed.ts",
    "stripe:seed": "ts-node ./scripts/stripe-seed.ts",
    "openapi:gen": "ts-node ./scripts/generate-openapi.ts",
    "pretest": "npm run prisma:gen",
    "test": "cross-env NODE_ENV=test OTEL_ENABLED=0 jest --config ./jest.config.ts --runInBand",
    "test:watch": "cross-env NODE_ENV=test OTEL_ENABLED=0 jest --config ./jest.config.ts --watch",
    "test:cov": "cross-env NODE_ENV=test OTEL_ENABLED=0 jest --config ./jest.config.ts --coverage --runInBand",
    "test:e2e": "cross-env NODE_ENV=test OTEL_ENABLED=0 jest --config ./jest-e2e.config.ts --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.2",
    "@nestjs/core": "^10.3.2",
    "@nestjs/platform-express": "^10.3.2",
    "@nestjs/config": "^3.2.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/swagger": "^7.3.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "yaml": "^2.4.5",
    "dotenv": "^16.4.5",
    "stripe": "^14.25.0",

    "@prisma/client": "5.16.1",
    "@prisma/instrumentation": "^6.1.0",

    "@opentelemetry/api": "1.8.0",
    "@opentelemetry/resources": "1.8.0",
    "@opentelemetry/semantic-conventions": "1.8.0",
    "@opentelemetry/sdk-node": "0.52.1",
    "@opentelemetry/auto-instrumentations-node": "0.52.1",
    "@opentelemetry/exporter-prometheus": "0.52.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/testing": "^10.3.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^3.0.10",
    "@types/supertest": "^2.0.16",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "prisma": "5.16.1",
    "supertest": "^6.3.3",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "overrides": {
    "@opentelemetry/api": "1.8.0",
    "@opentelemetry/resources": "1.8.0",
    "@opentelemetry/semantic-conventions": "1.8.0"
  }
}